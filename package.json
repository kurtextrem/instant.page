{
  "name": "instant.page",
  "version": "5.1.2",
  "description": "Make your site's pages instant in 1 minute and improve your conversion rate by 1%",
  "main": "dist/instantpage.min.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "minify": "terser instantpage.js --compress drop_console=true --mangle --toplevel --comments \"/^!/\" --output dist/instantpage.min.js",
    "build": "rm -r -f dist && mkdir dist && npm run minify",
    "lint": "exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/instantpage/instant.page.git"
  },
  "keywords": [
    "instant",
    "performance"
  ],
  "author": "Alexandre Dieulot, Jacob Gross",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kurtextrem/instant.page/issues"
  },
  "homepage": "https://instant.page/"
}
